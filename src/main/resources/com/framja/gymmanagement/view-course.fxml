<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style/Dashboard/member_dashboard.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.framja.gymmanagement.controller.ViewCourseController">
    <children>
        <VBox alignment="CENTER" prefHeight="27.0" prefWidth="600.0">
            <children>
                <Label fx:id="course_name" alignment="CENTER" text="Label">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Label>
            </children>
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </VBox>
        <VBox fx:id="gymClassContainer" alignment="TOP_CENTER" layoutX="10.0" layoutY="50.0" prefWidth="580.0" spacing="10.0">
            <!-- Cards will be dynamically added here -->
        </VBox>
    </children>
</AnchorPane>
